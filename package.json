{
  "version": "0.0.1",
  "description": "Hack4Climate app by Innosoft team",
  "main": "server/index.js",
  "scripts": {
    "start": "npm-run-all --parallel watch:server watch:build",
    "watch:build": "webpack --watch --config webpack.config.dev.js",
    "watch:server": "nodemon --watch server --exec babel-node -- server/index.js"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "nodemon": "^1.10.0",
    "npm-run-all": "^4.1.2",
    "react-hot-loader": "^1.3.0",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2"
  },
  "dependencies": {
    "express": "^4.14.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0"
  }
}
